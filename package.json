{
  "name": "troy-zada",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "emailjs-com": "^2.6.4",
    "react": "^18.3.1",
    "react-d3-cloud": "^1.0.6",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.24.1",
    "react-scripts": "^5.0.1",
    "react-transition-group": "^4.4.5",
    "web-vitals": "^4.2.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "css"
    ],
    "moduleNameMapper": {
      "\\.(css|png|jpg|jpeg)$": "<rootDir>/tests/__mocks__/MockStyles.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setupTests.js"
    ],
    "testEnvironment": "jsdom"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest ./tests",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
